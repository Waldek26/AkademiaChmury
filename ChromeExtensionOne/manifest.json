{
    "manifest_version":3,
    "name":"My Extension",
    "icons":{
        "32":"icons8-kawaii-broccoli-32.png",
        "48":"icons8-kawaii-broccoli-48.png"
    },
    "version": "1.0.0",
    "background": {
        "service_worker" : "service-worker.js",
        "type":"module"
    },
    "action": { 
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions":[
        "http://*/*",
        "https://*/*"
    ]
}